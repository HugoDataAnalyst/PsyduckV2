{
    "DUCKDB": {
        "max_memory_ram_gb": 2,
        "max_cpu_cores": 2,
        "duckdb_path": "/app/data/analytics.duckdb",
        "clean_pokemon_data_older_than_x_days": 30,
        "clean_raids_data_older_than_x_days": 30,
        "clean_quests_data_older_than_x_days": 30,
        "clean_invasions_data_older_than_x_days": 30
    },
    "SQL": {
        "store_sql_pokemon_aggregation": true,
        "store_sql_pokemon_shiny": true,
        "store_sql_raid_aggregation": true,
        "store_sql_quest_aggregation": true,
        "store_sql_invasion_aggregation": true
    },
    "IN-MEMORY":{
        "store_pokemon_timeseries": true,
        "store_pokemon_tth_timeseries": true,
        "store_raids_timeseries": true,
        "store_invasions_timeseries": true,
        "store_quests_timeseries": true
    },
    "REDIS": {
